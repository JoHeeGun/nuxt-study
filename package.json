{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxi dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "devDependencies": {
    "@nuxt/devtools": "^0.8.0",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@types/node": "^18.17.3",
    "nuxt": "^3.6.5"
  },
  "dependencies": {
    "@nuxt/kit": "^3.6.5",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/proxy": "^2.1.0",
    "@pinia/nuxt": "^0.4.11",
    "axios": "^1.4.0",
    "nuxt-swiper": "^1.2.0",
    "pinia": "^2.1.6",
    "swiper": "^10.1.0",
    "vue-router": "^4.2.4"
  },
  "eslintConfig": {
		"root": true,
		"env": {
			"node": true,
			"vue/setup-compiler-macros": true
		},
		"extends": [
			"plugin:vue/vue3-essential",
			"eslint:recommended"
		],
		"parserOptions": {
			"parser": "@babel/eslint-parser"
		},
		"rules": {
			"vue/multi-word-component-names": "off",
			"no-debugger": "off",
			"no-unused-vars": "off"
		}
	}
}
